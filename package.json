{
    "name": "npm-get-packages",
    "version": "1.0.2",
    "description": "Scan directories to find out all of dependencies and devDependencies of npm packages",
    "main": "bin/index.js",
    "files": [
        "!lib/__tests__/**/*",
        "lib/**/*",
        "bin/**/*"
    ],
    "bin": {
        "npm-get-packages": "bin/index.js",
        "ngp": "bin/index.js"
    },
    "scripts": {
        "start": "NODE_ENV=development ts-node ./src/index.ts",
        "build": "tsc",
        "serve": "node ./lib/index.js ",
        "clean": "rm -rf ./lib/",
        "prepare": "husky install",
        "test:watch": "jest --watch",
        "test": "jest --coverage",
        "type-check": "tsc --noEmit",
        "lint": "eslint \"./src/**/*.{js,,ts,,json}\"",
        "lint-fix": "eslint --fix \"./src/**/*.{js,,ts,,json}\"",
        "format": "prettier --write \"./src/**/*.{js,ts,json,md}\""
    },
    "license": "MIT",
    "engines": {
        "node": ">=19.0"
    },
    "dependencies": {
        "commander": "^11.0.0",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.3",
        "@types/node": "^20.4.1",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.6.1",
        "lint-staged": "^13.2.3",
        "prettier": "^3.0.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    },
    "directories": {
        "lib": "lib"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xSyki/npm-get-packages.git"
    },
    "keywords": [
        "seo",
        "tester",
        "site",
        "test",
        "web scraper"
    ],
    "author": "xSyki",
    "types": "./lib/index.d.ts",
    "bugs": {
        "url": "https://github.com/xSyki/npm-get-packages/issues"
    },
    "homepage": "https://github.com/xSyki/npm-get-packages#readme"
}
